set titlestring=''

set! javascript.options.jit.chrome=true

colorscheme sebpot

noremap <C-q> :quitall<CR>
noremap <C-x> :xall<CR>
noremap <C-t> :tabopen<CR>gh<CR>

noremap j 4j
noremap k 4k

inoremap <C-a> <C-Home><C-S-End>

" vim:ft=vim
